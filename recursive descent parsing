#include<stdio.h>
#include<stdlib.h>
#include<ctype.h>

char input[100];
int i = 0;

void E();
void Eprime();
void T();
void Tprime();
void F();

int main()
{
    printf("Enter the input string: ");
    scanf("%s", input);
    E();
    if(input[i] == '\0')
        printf("\nParsing Successful!\n");
    else
        printf("\nParsing Failed!\n");
    return 0;
}

void E()
{
    T();
    Eprime();
}

void Eprime()
{
    if(input[i] == '+')
    {
        i++;
        T();
        Eprime();
    }
}

void T()
{
    F();
    Tprime();
}

void Tprime()
{
    if(input[i] == '*')
    {
        i++;
        F();
        Tprime();
    }
}
